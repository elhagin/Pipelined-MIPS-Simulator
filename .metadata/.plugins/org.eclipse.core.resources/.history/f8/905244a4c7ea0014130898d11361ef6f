package pipelineRegisters;

public class EXMemReg {
	private M m;
	private WB wb;
	private int PCvalue;
	private int data;
	private int destination;
	private int result;
	private boolean isZero;

	public M getM() {
		return m;
	}

	public void setM(M m) {
		this.m = m;
	}

	public WB getWb() {
		return wb;
	}

	public void setWb(WB wb) {
		this.wb = wb;
	}

	public int getPCvalue() {
		return PCvalue;
	}

	public void setPCvalue(int pCvalue) {
		PCvalue = pCvalue;
	}

	public int getData() {
		return data;
	}

	public void setData(int data) {
		this.data = data;
	}

	public int getDestination() {
		return destination;
	}

	public void setDestination(int destination) {
		this.destination = destination;
	}

	public int getResult() {
		return result;
	}

	public void setResult(int result) {
		this.result = result;
	}

	public boolean isZero() {
		return isZero;
	}

	public void setZero(boolean isZero) {
		this.isZero = isZero;
	}
}
