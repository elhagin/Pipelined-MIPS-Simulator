package pipelineRegisters;

public class IFIDReg {
	private String instruction;
	private int PC;

	public IFIDReg() {
		super();

	}

	public String getInstruction() {
		return instruction;
	}

	public void setInstruction(String instruction) {
		this.instruction = instruction;
	}

	public int getPC() {
		return PC;
	}

	public void setPC(int pC) {
		PC = pC;
	}
}
