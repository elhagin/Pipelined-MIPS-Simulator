package pipelineRegisters;

import mainUnits.Simulator;

public class IDEXReg
{
	private static int memToReg;
	private static int branch;
	private static int aluOp;
	private static int aluSrc;
	private static int PC;
	private static int regData1;
	private static int regData2;
	private static int immediateConstant;
	private static String rt;
	private static String rd;
	
	public IDEXReg()
	{
		PC = Simulator.getiFID().getPC();
	}
	
	public int getMemToReg() {
		return memToReg;
	}
	public void setMemToReg(int memToReg) {
		this.memToReg = memToReg;
	}
	public int getBranch() {
		return branch;
	}
	public void setBranch(int branch) {
		this.branch = branch;
	}
	public int getAluOp() {
		return aluOp;
	}
	public void setAluOp(int aluOp) {
		this.aluOp = aluOp;
	}
	public int getAluSrc() {
		return aluSrc;
	}
	public void setAluSrc(int aluSrc) {
		this.aluSrc = aluSrc;
	}

	public int getPC() {
		return PC;
	}

	public void setPC(int pC) {
		PC = pC;
	}

	public int getRegData1() {
		return regData1;
	}

	public void setRegData1(int regData1) {
		this.regData1 = regData1;
	}

	public int getRegData2() {
		return regData2;
	}

	public void setRegData2(int regData2) {
		this.regData2 = regData2;
	}

	public String getInstruction() {
		return instruction;
	}

	public void setInstruction(String instruction) {
		this.instruction = instruction;
	}

	public int getImmediateConstant() {
		return immediateConstant;
	}

	public void setImmediateConstant(int immediateConstant) {
		this.immediateConstant = immediateConstant;
	}	
}
